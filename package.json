{
  "name": "framesjs-starter",
  "version": "0.2.6",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/dev-script.js",
    "dev:starter": "concurrently --kill-others \"next dev\" \"frames\"",
    "dev:monorepo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update-debugger": "npx degit github:framesjs/frames.js/examples/framesjs-starter/app/debug#main app/debug --force",
    "update-framesjs": "yarn upgrade frames.js@latest && yarn run update-debugger",
    "generate-migration": "yarn drizzle-kit generate:pg",
    "db:migrate": "tsx ./migrate.ts",
    "format": "prettier --write .",
    "db:studio": "pnpm drizzle-kit studio --port 3100"
  },
  "dependencies": {
    "@farcaster/core": "^0.14.3",
    "@neondatabase/serverless": "^0.9.0",
    "@noble/ed25519": "^2.0.0",
    "@privy-io/react-auth": "^1.59.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.28.6",
    "@vercel/kv": "^1.0.1",
    "@xmtp/frames-validator": "^0.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.4",
    "frames.js": "^0.9.3",
    "lucide-react": "^0.363.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-icons": "^5.0.1",
    "react-qr-code": "^2.0.12",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@frames.js/debugger": "^0.1.9",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
